{
    "name": "booking-tour",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "pre-commit": "lint-staged",
        "start": "nodemon --inspect ./bin/www",
        "watch": "node-sass --watch public/stylesheets/ --output public/assets/css/",
        "prepare": "husky install",
        "seed_user": "node ./config/db/seed/user",
        "seed_tour": "node ./config/db/seed/tour"
    },
    "lint-staged": {
        "*.{js,json,scss}": "prettier --single-quote --trailing-comma all --tab-width 4 --write"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "bower": "^1.3.12",
        "concat-cli": "^4.0.0",
        "cookie-parser": "~1.4.4",
        "date-and-time": "^1.0.1",
        "debug": "~2.6.9",
        "dotenv": "^10.0.0",
        "express": "~4.16.1",
        "express-handlebars": "^5.3.2",
        "express-session": "^1.17.2",
        "faker": "^5.5.3",
        "http-errors": "~1.6.3",
        "jquery-datetimepicker": "^2.5.21",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.13.2",
        "mongoose-seed": "^0.6.0",
        "mongoose-sequence": "^5.3.1",
        "mongoose-slug-generator": "^1.0.4",
        "morgan": "~1.9.1",
        "nodemailer": "^6.6.2",
        "nodemailer-express-handlebars": "^4.0.0",
        "uglifycss": "0.0.29"
    },
    "devDependencies": {
        "lint-staged": "^11.0.0",
        "node-sass": "^6.0.1",
        "nodemon": "^2.0.9",
        "prettier": "^2.3.2",
        "husky": "^7.0.0"
    }
}
